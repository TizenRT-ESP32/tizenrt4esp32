config EXAMPLES_TASK_MANAGER_SAMPLE
	bool "Task Manager Sample"
	default n
	depends on TASK_MANAGER
	---help---
		Enable the Task Manager Sample

config USER_ENTRYPOINT
	string
	default "task_manager_sample_main" if ENTRY_TASK_MANAGER_SAMPLE
